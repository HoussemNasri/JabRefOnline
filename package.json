{
  "name": "nuxt-express",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "yarn prisma:generate && yarn graphql:generate",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:migrate:deploy": "prisma migrate deploy --preview-feature",
    "prisma:migrate:status": "prisma migrate status --preview-feature",
    "prisma:migrate:resolve": "prisma migrate resolve --preview-feature",
    "prisma:seed": "prisma db seed --preview-feature",
    "prisma:studio": "prisma studio",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "prisma generate --watch",
    "graphql:generate": "graphql-codegen --config codegen.yml",
    "lint": "yarn lint:js",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "postinstall": "yarn generate",
    "test": "jest"
  },
  "dependencies": {
    "@nuxt/http": "^0.6.4",
    "@prisma/client": "^2.18.0",
    "apollo-server-express": "^2.21.0",
    "bcryptjs": "^2.4.3",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "graphql": "^15.5.0",
    "graphql-passport": "^0.6.3",
    "nuxt": "^2.15.2",
    "passport": "^0.4.1",
    "typescript": "^4.2.3",
    "webpack": "^5.24.4"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.21.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@nuxt/types": "^2.15.2",
    "@nuxt/typescript-build": "^2.0.6",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/eslint-module": "^3.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/passport": "^1.0.6",
    "eslint": "^7.20.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-nuxt": "^2.0.0",
    "nuxt-vite": "^0.0.28",
    "prisma": "2.18.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@vue/test-utils": "^1.1.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.5.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.4.4",
    "vue-jest": "^3.0.4"
  }
}
